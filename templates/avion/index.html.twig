<!DOCTYPE html>
<html lang="en">
{% include 'head.html.twig' %}
<body>




<section>
    <div>
<table class="table" id="avion">
                <thead>
                <tr>
                    <th>Id</th>
                    <th>Nom</th>
                    <th>Actions</th>
                </tr>
                </thead>
                <tbody>
                {% for avion in avions %}
                    <tr>
                        <td>{{ avion.id }}</td>
                        <td>{{ avion.nom }}</td>
                        <td>
                            <a href="{{ path('app_avion_show', {'id': avion.id}) }}">Show</a>
                            <a href="{{ path('app_avion_edit', {'id': avion.id}) }}">Edit</a>
                        </td>
                    </tr>
                {% else %}
                    <tr>
                        <td colspan="3">No records found</td>
                    </tr>
                {% endfor %}
                </tbody>
            </table>

            <a href="{{ path('app_avion_new') }}">Create new</a>
            <a href="{{ path('app_index') }}">Retour</a>
        <a href="{{ path('app_modele_new') }}">Créer un modèle</a>
        </div>

</section>



</body>

</html>
<style>
body {
padding-top:300px
}

</style>